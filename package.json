    "size-limit": "^7.0.8",
    "ts-jest": "^27.1.1",
    "tsconfig": "*",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4",
